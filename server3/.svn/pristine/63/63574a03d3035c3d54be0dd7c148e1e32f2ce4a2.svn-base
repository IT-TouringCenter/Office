<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class account_request_history extends baseModel
{
    protected $table = "account_request_histories";

    public function accounts(){
    	return $this->belongsTo('App/account','account_id');
    }
    public function account_email_historys(){
    	return $this->hasMany('App/account_email_history','account_request_history_id');
    }
    public function account_request_types(){
    	return $this->belongsTo('App/account_request_type','account_request_type_id');
    }
}
