<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class hotel extends baseModel
{
	protected $table = "hotels";
	protected $hidden = ['address','phone','fax','is_active','created_by','updated_by','created_at','updated_at'];

	public function passengers(){
		return $this->hasMany('App/passenger','hotel_id');
	}
	public function accounts(){
		return $this->hasMany('App/account','hotel_id');
	}
	public function activities(){
		return $this->belongTo('App/activity', 'activity_id');
	}
}
