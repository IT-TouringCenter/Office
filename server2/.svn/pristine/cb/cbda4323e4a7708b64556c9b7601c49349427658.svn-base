<?php 
namespace App\Repositories;

use App\Repositories\Src\Contracts\IRepository;
use App\Repositories\Src\Eloquent\Repository;

class JobDepartmentRepository extends Repository{
  function model(){return "App\job_department";}

  public function GetDepartmentByInterns(){
  	return $this->model
  				->where('is_active',1)
  				->where('job_type_id',4)
  				->get();
  }
}