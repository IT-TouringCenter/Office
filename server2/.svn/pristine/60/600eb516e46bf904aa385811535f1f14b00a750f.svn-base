<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class holiday extends baseModel
{
    protected $table = "holidays";

    public function tour_programs(){
    	return $this->belongsTo('App/tour_program','tour_program_id');
    }
    public function holiday_types(){
    	return $this->belongsTo('App/holiday_type','holiday_type_id');
    }
}
