<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class configuration_account_permission extends baseModel
{
    protected $table = "configuration_account_permissions";

    public function accounts(){
    	return $this->belongsTo('App/account','account_id');
    }
    public function account_permissions(){
    	return $this->belongsToMany('App/account_permission','account_permission_id');
    }
    public function myaccount_left_menus(){
    	return $this->belongsToMany('App/myaccount_left_menu','myaccount_left_menu_id');
    }
}
