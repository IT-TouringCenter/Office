<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class tour_program extends baseModel
{    
    protected $table = "tour_programs";

    public function configuration_tour_prices(){
    	return $this->hasMany('App/configuration_tour_price','tour_program_id');
    }
    public function holidays(){
    	return $this->hasMany('App/holiday','tour_program_id');
    }
    public function notify_report_cronjobs(){
    	return $this->hasMany('App/notify_report_cronjob','tour_program_id');
    }
    public function configuration_discounts(){
    	return $this->hasMany('App/configuration_discount','tour_program_id');
    }
    public function order_change_historys(){
    	return $this->hasMany('App/order_change_history','tour_program_id');
    }
    // public function order_items(){
    // 	return $this->hasMany('App/order_item','tour_program_id');
    // }
    public function tour_traveling_times(){
        return $this->hasOne('App/tour_traveling_time','tour_program_id');
    }
    // public function tour_categorys(){
    //     return $this->belongsTo('App/tour_category','tour_category_id');
    // }
    // public function tour_types(){
    //     return $this->belongsTo('App/tour_type','tour_type_id');
    // }
}
