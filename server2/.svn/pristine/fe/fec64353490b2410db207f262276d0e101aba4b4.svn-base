<?php namespace App;

use Illuminate\Database\Eloquent\Model;

class payment_transaction extends baseModel {

	protected $table = "payment_transactions";

    public function payment_transaction_statuses(){
    	return $this->belongsTo('App/payment_transaction_status','payment_transaction_status_id');
    }

    public function Transaction(){
        return $this->belongsTo('App\transactions','transaction_id');
    }

}
