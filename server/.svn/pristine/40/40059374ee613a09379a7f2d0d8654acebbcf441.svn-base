<?php 
namespace App\Repositories;

use App\Repositories\Src\Contracts\IRepository;
use App\Repositories\Src\Eloquent\Repository;

class HotelRepository extends Repository{
  function model(){return "App\hotel";}

  public function GetPartnerHotel(){
  	return $this->model
			  	->where('is_active',1)
			  	->where('is_conceal',0)
			  	->where('is_partner',1)
			  	->get(['id','hotel']);
  }
}