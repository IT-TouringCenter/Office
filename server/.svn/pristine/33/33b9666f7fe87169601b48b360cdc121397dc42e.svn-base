<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class notify_status_cronjob extends baseModel
{
    protected $table = "notify_status_cronjobs";

    public function notify_report_cronjobs(){
    	return $this->hasMany('App/notify_report_cronjob','notify_status_cronjob_id');
    }
    public function notify_promotion_code_cronjobs(){
    	return $this->hasMany('App/notify_promotion_code_cronjob','notify_status_cronjob_id');
    }
    public function notify_order_cronjobs(){
    	return $this->hasMany('App/notify_order_cronjob','notify_status_cronjob_id');
    }
}
