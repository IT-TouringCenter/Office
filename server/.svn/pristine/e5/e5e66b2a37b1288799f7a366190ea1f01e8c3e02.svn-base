<?php namespace App;

use Illuminate\Database\Eloquent\Model;

class transaction_transfer_airport extends Model {

	protected $table = 'transaction_transfer_airports';

	public function Flight(){
		return $this->belongsTo("App\airport_origin",'flight_origin_id');
	}

	public function Person(){
		return $this->belongsTo('App\passenger','passenger_id');
	}
}
