<?php
namespace App\Facades\Tours;

use App\Repositories\TourTravelingTimeRepository as TourTravelingTimeRepository;

class TourTravelingTimeClass extends BaseClass{
  public function __construct(TourTravelingTimeRepository $repo){
    $this->TourTravelingTimeRepository = $repo;
  }

  public function GetTourTravelingTimeById($tour_program_id){
    $traveling_time_arr = [];
    $traveling_time = $this->TourTravelingTimeRepository->findBy('tour_program_id',$tour_program_id,['id','traveling_time']);
    return $traveling_time;
  }
}

