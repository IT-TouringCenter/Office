<?php
namespace App\Facades\Tours;

use App\Repositories\JobTypeRepository as JobTypeRepository;

class JobTypeClass extends BaseClass{  
  public function __construct(JobTypeRepository $repo){
    $this->JobTypeRepository = $repo;
  }

  public function GetJobType($type_id){
  	$job_type_repo = $this->JobTypeRepository->findBy('id',$type_id);
  	
    return $job_type_repo;
  }
}

?>
